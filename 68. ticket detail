#include <stdio.h>

struct Ticket {
    int ticketNo;
    char passenger[30];
    float fare;
};

int main() {
    struct Ticket t;

    printf("Enter Ticket No: ");
    scanf("%d", &t.ticketNo);
    printf("Enter Passenger Name: ");
    scanf("%s", t.passenger);
    printf("Enter Fare: ");
    scanf("%f", &t.fare);

    printf("\nTicket Details:\n");
    printf("Ticket No: %d\nPassenger: %s\nFare: %.2f\n",
           t.ticketNo, t.passenger, t.fare);
    return 0;
}
