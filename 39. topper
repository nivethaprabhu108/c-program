#include <stdio.h>

int main() {
    int n, i, marks[50], top, pos = 0;

    printf("Enter number of students: ");
    scanf("%d", &n);

    for (i = 0; i < n; i++) {
        scanf("%d", &marks[i]);
    }

    top = marks[0];
    for (i = 1; i < n; i++) {
        if (marks[i] > top) {
            top = marks[i];
            pos = i;
        }
    }

    printf("Topper is student %d with marks %d", pos + 1, top);
    return 0;
}
